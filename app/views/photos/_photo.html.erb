<% if @photo.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@photo.errors.count, "error") %>
      prohibited this account from being created:
    </h2>

    <ul>
    <% @photo.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>


<%= form_for @photo do |f| %>
  Title: <%= f.text_field :title %>
  Url: <%= f.text_field :url %><br><br>
  <%= f.submit 'Upload Photo' %>
<% end %>